<h1>Listing submissions</h1>

<table>
  <tr>
    <th>Song title</th>
    <th>Artist</th>
    <th>Submitted by</th>
    <th>Vid</th>
    <th>Video service</th>
    <th>Pros</th>
    <th>Cons</th>
    <th>Selection session</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @submissions.each do |submission| %>
  <tr>
    <td><%= submission.song_title %></td>
    <td><%= submission.artist %></td>
    <td><%= submission.submitted_by_id %></td>
    <td><%= submission.vid %></td>
    <td><%= submission.video_service %></td>
    <td><%= submission.pros %></td>
    <td><%= submission.cons %></td>
    <td><%= submission.selection_session_id %></td>
    <td><%= link_to 'Show', submission %></td>
    <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
    <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Submission', new_submission_path %>
